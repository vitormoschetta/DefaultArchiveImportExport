@model IEnumerable<DefaultArchiveImportExport.Models.Product>


<table class="table">
    <thead>
         <tr>
            <td>Id</td>
            <td>Name</td>
            <td>Price</td>
         </tr>
    </thead>

    <tbody>
   
    @foreach (var item in Model)
    {
        
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Price</td>
        </tr>
        
    
    }
    </tbody>

</table>

